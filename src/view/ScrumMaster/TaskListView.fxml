<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="490.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ScrumMaster.TaskListViewController">
   <children>
      <TableView layoutY="111.0" prefHeight="253.0" prefWidth="529.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="title" prefWidth="75.0" text="title" />
            <TableColumn fx:id="timeSpent" prefWidth="75.0" text="time spent" />
            <TableColumn fx:id="timeEstimeted" prefWidth="105.0" text="time estimeted" />
            <TableColumn fx:id="responsiblePerson" prefWidth="119.0" text="resposible person" />
            <TableColumn fx:id="status" prefWidth="75.0" text="status" />
        </columns>
      </TableView>
      <Label layoutX="251.0" layoutY="23.0" />
      <HBox fx:id="titleLabel" alignment="CENTER" layoutX="6.0" prefHeight="100.0" prefWidth="529.0" spacing="20.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Requirement:">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
            <Label text="title of requirement">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="6.0" layoutY="364.0" prefHeight="100.0" prefWidth="522.0" spacing="50.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#backOnClick" prefWidth="120.0" text="Back">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="add" mnemonicParsing="false" onAction="#addOnClick" prefWidth="120.0" text="Add">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="delete" mnemonicParsing="false" onAction="#deleteOnClick" prefWidth="120.0" text="Delete">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
