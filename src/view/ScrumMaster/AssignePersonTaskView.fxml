<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="210.0" prefWidth="447.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ScrumMaster.AssignePersonTaskViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="85.0" prefWidth="434.0" spacing="20.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name:">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
            <TextField fx:id="name">
               <font>
                  <Font size="25.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="85.0" prefHeight="100.0" prefWidth="434.0" spacing="80.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#backOnClick" prefWidth="120.0" text="Back">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="Assign" mnemonicParsing="false" onAction="#assigneOnClick" prefWidth="120.0" text="Assign">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
