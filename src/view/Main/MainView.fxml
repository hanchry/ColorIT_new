<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Main.MainViewController">
   <children>
      <HBox alignment="CENTER" layoutY="7.0" prefHeight="100.0" prefWidth="600.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Select a role">
               <font>
                  <Font size="40.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="107.0" prefHeight="293.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" prefHeight="293.0" prefWidth="300.0" spacing="50.0">
               <children>
                  <Button fx:id="ProjectCreato" mnemonicParsing="false" onAction="#ProjectCreatorOnClick" prefHeight="51.0" prefWidth="193.0" text="Project Creator">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button fx:id="ScrumMaster" mnemonicParsing="false" onAction="#ScrumMasterOnClick" prefHeight="51.0" prefWidth="193.0" text="Scrum Master">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="160.0" layoutY="10.0" prefHeight="293.0" prefWidth="300.0" spacing="50.0">
               <children>
                  <Button fx:id="ProjectOwner" mnemonicParsing="false" onAction="#ProjectOwnerOnCLick" prefHeight="51.0" prefWidth="189.0" text="Project Owner">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button fx:id="Developer" mnemonicParsing="false" onAction="#DeveloperOnCLick" prefHeight="51.0" prefWidth="189.0" text="Developer">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
