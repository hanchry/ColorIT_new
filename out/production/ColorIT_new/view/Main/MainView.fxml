            <VBox alignment="CENTER" prefHeight="263.0" prefWidth="300.0" spacing="50.0">
               <children>
                  <Button fx:id="ProjectCreator" mnemonicParsing="false" onAction="#ProjectCreatorOnClick" prefHeight="51.0" prefWidth="193.0" text="Project Creator">
